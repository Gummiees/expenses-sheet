{
  "name": "expenses-sheet",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "emulate": "firebase emulators:start",
    "test": "ng test",
    "check:es": "ng lint --fix",
    "check:styles": "stylelint \"src/**/*.scss\"",
    "check": "npm run check:es && npm run check:styles",
    "watch": "ng build --watch --configuration development",
    "build": "ng build",
    "buildpro": "ng build --configuration production",
    "deploy": "firebase deploy",
    "commit": "npm run check && git add . && git commit -m \"auto-commit\"",
    "push": "npm run commit && git push"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.7",
    "@angular/cdk": "^14.2.5",
    "@angular/common": "^14.2.7",
    "@angular/compiler": "^14.2.7",
    "@angular/core": "^14.2.7",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^14.2.7",
    "@angular/material": "^14.2.5",
    "@angular/material-moment-adapter": "^14.2.7",
    "@angular/platform-browser": "^14.2.7",
    "@angular/platform-browser-dynamic": "^14.2.7",
    "@angular/router": "^14.2.7",
    "firebase": "^9.12.1",
    "moment": "^2.29.4",
    "primeicons": "^6.0.1",
    "primeng": "^14.1.2",
    "rxjs": "^7.5.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "^14.2.6",
    "@angular/compiler-cli": "^14.2.7",
    "@types/file-saver": "^2.0.5",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-saver": "^2.0.5",
    "firebase-tools": "^11.15.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "postcss": "^8.4.18",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "rxfire": "^6.0.3",
    "stylelint": "^14.14.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.7.4",
    "xlsx": "^0.18.5"
  }
}
